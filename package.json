{
  "name": "sma-dashboard",
  "description": "Social Media Analytics Dashboard with React & Redux ",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@material/checkbox": "^0.25.0",
    "@material/drawer": "^0.34.0",
    "@material/snackbar": "^0.30.0",
    "@material/textfield": "^0.3.6",
    "gradient-color": "^2.0.1",
    "highcharts": "^6.0.4",
    "material-components-web": "^0.30.0",
    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "rc-menu": "^6.2.10",
    "react": "^15.6.2",
    "react-color": "^2.14.1",
    "react-dom": "^15.6.2",
    "react-highcharts": "^15.0.0",
    "react-input-colorpicker": "^1.0.13",
    "react-loader-advanced": "^1.7.1",
    "react-mdc-web": "^0.17.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "0.9.5",
    "react-select": "^1.2.1",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sort-json-array": "^0.1.7",
    "sweetalert-react": "^0.4.11"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ ",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "jest --coverage",
    "test:watch": "npm test --watch",
    "eject": "react-scripts eject"
  },
  "author": "Makan Singh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git.xenonstack.com"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.2.1",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.2.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "object.assign": "^4.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "regenerator-runtime": "^0.11.1",
    "sinon": "^4.0.1",
    "webpack-dev-server": "^2.7.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./config/setupTest.js"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|png|svg|gif)$": "<rootDir>/test-config/fileMock.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      },
      "./coverage/lcov-report/components/**/*.js": {
        "branches": 40,
        "statements": 40
      },
      "./coverage/lcov-report/reducers/**/*.js": {
        "statements": 80
      },
      "./coverage/lcov-report/api/very-important-module.js": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
